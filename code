
#include <Servo.h> // servo library
Servo myservo; // servo name
int pos = 0; // initial servo position (0=0)


//
void setup() {
  
    // initialization code, runs once in entire circuit
    
    myservo.attach(11); // servo on pin 11
    Serial.begin(9600); // serial for reading
    
    //all loops below are time-framed (=ms), and the servowrite function represents a "shaking" motion, that varies in timecycle
    
}

